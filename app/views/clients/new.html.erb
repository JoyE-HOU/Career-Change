<h1>New Client</h1>

<%= form_for @client do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br/>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <br/>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
    <br/>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br/>

    <%= f.submit %>
    <%end%>